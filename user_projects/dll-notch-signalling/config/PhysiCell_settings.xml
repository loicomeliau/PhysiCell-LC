                    <cfg_filename>config/dll-notch.cfg</cfg_filename>
                    <initial_values>
                        <initial_value intracellular_name="VEGF">0</initial_value>
                        <initial_value intracellular_name="NOTCH">0</initial_value>
                    </initial_values>
                    <settings>
                        <intracellular_dt>1</intracellular_dt>
                        <time_stochasticity>0.5</time_stochasticity>
                        <scaling>1.0</scaling>
                        <start_time>0.0</start_time>
                        <inheritance global="False" />
                    </settings>
                    <mapping>
                        <input physicell_name="VEGF" intracellular_name="VEGF">
                            <settings>
                                <action>activation</action>
                                <threshold>1</threshold>
                                <smoothing>5</smoothing>
                            </settings>
                        </input>
                        <input physicell_name="custom:dll_neighbour" intracellular_name="DLL_neighbour">
                            <settings>
                                <action>activation</action>
                                <threshold>0.49</threshold>
                                <inact_threshold>0.51</inact_threshold>
                                <smoothing>5</smoothing>
                            </settings>
                        </input>
                    </mapping>
